//Writing on a text file
#include<iostream>
#include<fstre

using namespace std;
int main(){
  ofstream myfile("example.txt");
  if (myfile.is_open())
    {
      myfile<<"This is a line.\n";
      myfile<<"This is another line.\n";
      myfile.close();
    }
  else cout<<"Unalble to open file";
  return 0;
}
